<odoo>
    <data>
        <record id="intn_trazabilidad_uso_marca.imprimir_wizard_view" model="ir.ui.view">
            <field name="name">intn_trazabilidad_uso_marca.imprimir_wizard_view</field>
            <field name="model">impresion.etiquetas.imprimir.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="impresion_etiquetas_id" readonly="True"/>
                            <field name="nro_control"/>
                            <field name="lote_etiqueta"/>
                            <field name="cantidad"/>
                        </group>
                    </group>
                    <footer>
                        <button string="Imprimir" type="object" name="button_imprimir" class="oe_highlight"/>
                        <button string="Cancelar" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
